{
  "name": "online-chatroom",
  "version": "1.0.0",
  "description": "A simple chatroom",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "client-install": "npm install --prefix chatrooms",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run serve --prefix chatrooms",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "chatroom"
  ],
  "author": "yanpeng",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.3.0"
  }
}
